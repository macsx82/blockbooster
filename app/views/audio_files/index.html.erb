<h1>Listing audio_files</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Album</th>
    <th>Track number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @audio_files.each do |audio_file| %>
  <tr>
    <td><%= audio_file.title %></td>
    <td><%= audio_file.author_id %></td>
    <td><%= audio_file.album_id %></td>
    <td><%= audio_file.track_number %></td>
    <td><%= link_to 'Show', audio_file %></td>
    <td><%= link_to 'Edit', edit_audio_file_path(audio_file) %></td>
    <td><%= link_to 'Destroy', audio_file, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Audio file', new_audio_file_path %>
